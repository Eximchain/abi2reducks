import { MethodAbi } from 'ethereum-types';

export enum {{ titleName }}Params {
{{#each inputs}}
{{#if name}}
    {{name}} = '{{name}}',
{{else}}
    param{{@index}} = 'param{{@index}}',
{{/if}}
{{/each}}
}

export type {{ titleName }}Input = {
{{#each inputs}}
{{#if name}}
    {{name}} : {{solToJsType type}}
{{else}}
    param{{@index}} : {{solToJsType type}}
{{/if}}
{{/each}}
}

export const {{ titleName }}Method:MethodAbi = {{{ stringify methodAbi }}}